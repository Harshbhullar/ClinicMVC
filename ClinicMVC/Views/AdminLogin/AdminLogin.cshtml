
@{
    ViewBag.Title = "AdminLogin";
}



<section class="wthree-row w3-contact py-lg-5">
    <div class="container py-4">
        <div class="text-center wthree-title pb-sm-5 pb-3">
            <h3 class="w3l-sub">Admin Login Portal </h3>
            
        </div>
        <!-- contact details -->
        <div class="row contact-form py-lg-5">
            <!-- contact left grid -->
            <!-- //contact left grid -->
            <!-- contact right grid -->
            <div class="col-lg-12 wthree-form-left mt-lg-0 mt-3">
                <!-- contact form grid -->
                <div class="contact-top1">
                    <h5 class="my-3">Admin Login</h5>
                    <form method="post" action="Login">

                        <div class="form-group my-4">
                            <label for="contactemail">Email</label>
                            <input type="text" id="Name" name="Name" class="form-control" required />


                        </div>
                        <div class="form-group">
                            <label for="contactusername">Password</label>
                            <input type="password" id="Password" name="Password" class="form-control" required />

                        </div>
                        <input type="submit" name="msg" id="msg" value="send Message" />

                    </form>


                    <h1 id="data" runat="server"> </h1>
                </div>
            </div>
            <!--  //contact right grid -->
        </div>
        <!-- //contact details  -->
    </div>
</section>
<!-- //contact -->